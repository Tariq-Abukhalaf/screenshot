<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title></title>
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <meta name="robots" content="noindex" />
        <link rel="stylesheet" type="text/css" href="plugins/bootstrap/css/bootstrap.min.css" />
        <link rel="stylesheet" type="text/css" href="plugins/select2/css/select2.min.css" />
        <link rel="stylesheet" type="text/css" href="plugins/Ionicons/css/ionicons.min.css" />
        <link rel="stylesheet" type="text/css" href="plugins/adminlte/css/AdminLTE.min.css" />
        <link rel="stylesheet" type="text/css" href="plugins/adminlte/css/skins/_all-skins.min.css" />
        <link rel="stylesheet" type="text/css" href="plugins/iCheck/all.css" />
        <link rel="stylesheet" type="text/css" href="css/custom.css" />
        <link rel="stylesheet" type="text/css" href="css/style.css" />
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" />
    </head>
    <body class="sidebar-mini skin-black-light">
        <h1>EC2 slaves Instances</h1>
        <table class="table table-bordered table-dark">
            <thead>
              <tr>
                <th>ImageId</th>
                <th>InstanceId</th>
                <th>InstanceType</th>
                <th>State</th>
                <th>PrivateIpAddress</th>
                <th>PublicIpAddress</th>
                <th>PlatformDetails</th>
              </tr>
            </thead>
            <tbody>
              <% listSlaveInstances.forEach(instance => { %>
                <tr>
                  <td><%= instance.ImageId %></td>
                  <td><%= instance.InstanceId %></td>
                  <td><%= instance.InstanceType %></td>
                  <td><%= instance.State %></td>
                  <td><%= instance.PrivateIpAddress || 'N/A' %></td>
                  <td><%= instance.PublicIpAddress || 'N/A' %></td>
                  <td><%= instance.PlatformDetails  %></td>
                </tr>
              <% }); %>
            </tbody>
        </table>

        <script type="text/javascript" src="plugins/jquery/jquery.min.js"></script>
        <script type="text/javascript" src="plugins/bootstrap/js/bootstrap.min.js"></script>
        <script type="text/javascript" src="plugins/select2/js/select2.full.min.js"></script>
        <script type="text/javascript" src="plugins/jquery-slimscroll/jquery.slimscroll.min.js"></script>
        <script type="text/javascript" src="plugins/iCheck/icheck.min.js"></script>
        <script type="text/javascript" src="plugins/fastclick/lib/fastclick.js"></script>
        <script type="text/javascript" src="plugins/adminlte/js/adminlte.min.js"></script>
        <script type="text/javascript" src="js/custom.js"></script>
        <script type="text/javascript" src="js/validate.js"></script>
        <script type="text/javascript" src="js/bootbox.min.js"></script>
    </body>
</html>
